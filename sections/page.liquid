{% comment %}
  This is the main template for custom pages in your Shopify theme.
  It renders the content of the page and allows for custom sections or blocks.
{% endcomment %}

<section class="page-section">
  <div class="container">
    <h1 class="page-title">{{ page.title }}</h1>
    <div class="page-content rte">
      {{ page.content }}
    </div>
  </div>
</section>
